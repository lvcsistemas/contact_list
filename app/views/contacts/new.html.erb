<div class="row">
    <div class="col-xs-12">
        <h1>Novo Contato</h1>
    </div>
    <div class="col-md-6 col-md-offset-3 cols-xs-12">
        <%= form_for @contact do |f| %>
            <%= render 'form', f: f %>
            <%= f.submit 'Salvar', class: 'btn btn-primary' %>
        <% end %>
    </div>
</div>